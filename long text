                if executor is console:
                    set {_silent} to "true"
                    replace all " -s" with "" in {ipmutereason::%{ip::%{uuid::%{arg-1}%}%}%}
            if {ipmutereason::%{ip::%{uuid::%{arg-1}%}%}%} contains "-s":
                if player has permission "ban.silent":
                    set {_silent} to "true"
                    replace all "-s" with "" in {ipmutereason::%{ip::%{uuid::%{arg-1}%}%}%}
                if executor is console:
                    set {_silent} to "true"
                    replace all "-s" with "" in {ipmutereason::%{ip::%{uuid::%{arg-1}%}%}%}
            delete {accs}
            loop {alts::%{ip::%{uuid::%{arg-1}%}%}%::*}:
