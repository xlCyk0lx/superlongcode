            log "[PUNISHED] %player% IP-banned %{accs}% for %{ipduration::%{ip::%{uuid::%{arg-1}%}%}%}%. Reason: %{ipreason::%{ip::%{uuid::%{arg-1}%}%}%}%" to "%uuid of player%/punishments-given.log"
